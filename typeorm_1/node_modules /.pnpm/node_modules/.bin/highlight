#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/bin/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules/cli-highlight/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/cli-highlight@2.1.11/node_modules:/Users/sarojdahal/Desktop/node-ts-backend-batch1/typeorm_setup/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cli-highlight/bin/highlight" "$@"
else
  exec node  "$basedir/../cli-highlight/bin/highlight" "$@"
fi
